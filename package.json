{
  "name": "log-manager",
  "version": "1.0.0",
  "description": "Log manager to aggregate logs across servers in a single spot.",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc && cd client && npm install && npm run build",
    "prestart": "npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon .\" \"cd client && npm start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MitchStark10/log-manager.git"
  },
  "author": "Mitch Stark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MitchStark10/log-manager/issues"
  },
  "homepage": "https://github.com/MitchStark10/log-manager#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/mysql": "^2.15.18",
    "@types/node": "^15.12.5",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.29.0",
    "nodemon": "^2.0.10",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  }
}
