{
  "name": "log-manager",
  "version": "1.1.0",
  "description": "Log manager to aggregate logs across servers in a single spot.",
  "main": "dist/server.js",
  "scripts": {
    "copyEnv": "cp .env ./dist/.env",
    "build": "npx tsc && cd client && npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cp .env ./dist/ && concurrently --kill-others \"tsc -w\" \"nodemon .\" \"cd client && npm start\"",
    "postinstall": "mkdir -p dist && cd client && npm i",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MitchStark10/log-manager.git"
  },
  "author": "Mitch Stark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MitchStark10/log-manager/issues"
  },
  "homepage": "https://github.com/MitchStark10/log-manager#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.0.2",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.2",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.178",
    "@types/mysql": "^2.15.18",
    "@types/node": "^15.12.5",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.10",
    "prompt-sync": "^4.2.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "**.{ts,tsx}": "npm run lint"
  }
}